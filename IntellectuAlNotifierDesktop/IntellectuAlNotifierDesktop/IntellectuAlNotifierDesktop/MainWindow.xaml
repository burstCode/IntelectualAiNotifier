<Window x:Class="IntellectuAlNotifierDesktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IntellectuAlNotifierDesktop"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" MinWidth="400" WindowState="Maximized">
    <Window.FontFamily>
        <FontFamily>Ubuntu</FontFamily>
    </Window.FontFamily>
    <Window.FontWeight>
        <FontWeight>Bold</FontWeight>
    </Window.FontWeight>
    <Grid>
        <Viewbox HorizontalAlignment="Left">
            <local:SidePanel></local:SidePanel>
        </Viewbox>
    </Grid>
</Window>
